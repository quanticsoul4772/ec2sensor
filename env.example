# EC2 Sensor Configuration
# Copy this file to .env and update with your values

# API Configuration (get from your team)
EC2_SENSOR_API_KEY=your_api_key_here
EC2_SENSOR_BASE_URL=https://your-api-endpoint.execute-api.us-east-1.amazonaws.com/prod/ec2_sensor

# Current Sensor (optional - will be set automatically)
SENSOR_NAME=

# Default Configuration
DEFAULT_BRANCH=testing
DEFAULT_TEAM=your_team
DEFAULT_USERNAME=your_username

# SSH Configuration (get from your team)
SSH_USERNAME=broala
SSH_PASSWORD=your_ssh_password_here

# Admin Password (for P1 automation preparation)
ADMIN_PASSWORD=your_admin_password_here
